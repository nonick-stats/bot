{
	"name": "nonick.stats",
	"version": "1.0.0",
	"description": "Minecraftサーバーの統計を表示するBOT",
	"main": "index.js",
	"scripts": {
		"start": "node build/index.js",
		"dev": "nodemon --inspect src/index.ts",
		"reset": "node --loader ts-node/esm src/resetCommands.ts",
		"build": "tsc -p . && pnpm run copy-assets",
		"copy-assets": "shx cp -r src/fonts build/fonts && shx cp -r src/images build/images"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@akki256/discord-interaction": "^3.1.1",
		"@biomejs/biome": "^1.5.3",
		"@napi-rs/canvas": "^0.1.41",
		"axios": "^1.6.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.2.0",
		"mongoose": "^7.8.3",
		"typescript": "^4.9.5"
	},
	"devDependencies": {
		"@swc/core": "^1.3.64",
		"@swc/helpers": "^0.4.14",
		"nodemon": "^2.0.22",
		"shx": "^0.3.4",
		"ts-node": "^10.9.1"
	},
	"override": {
		"discord-api-types": "0.37.35"
	},
	"pnpm": {
		"override": {
			"discord-api-types": "0.37.35"
		}
	}
}
